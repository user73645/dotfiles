URxvt.font: xft:Office code pro:size=11
URxvt.scrollBar: false

! transparency
URxvt*inheritPixmap:		true
URxvt*transparent:		true
URxvt*shading:			30

! clickable links
URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1

! URxvt.colorIT:		#87af5f
! URxvt.colorBD:		#d7d7d7
! URxvt.colorUL:		#87afd7

! special
*.foreground:   #e9e9e9
*.background:   #000000
*.cursorColor:  #e9e9e9

! black
*.color0:       #ae7b7b
*.color8:       #e9e9e9

! red
*.color1:       #c10023
*.color9:       #ff8781

! green
*.color2:       #a83334
*.color10:      #ffa095

! yellow
*.color3:       #b51d2c
*.color11:      #ff948b

! blue
*.color4:       #8c4b46
*.color12:      #feb2aa

! magenta
*.color5:       #7d534f
*.color13:      #ebbbb5

! cyan
*.color6:       #9a413d
*.color14:      #ffaaa0

! white
*.color7:       #c6c6c6
*.color15:      #ffffff
